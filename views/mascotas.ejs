<%- include("template/head", {tituloWeb: "PÃ¡gina de mascotas"}) %>
<body>
    <h1 class="btn-danger w-50 mx-auto">Aqui van las mascotas</h1>
   

    <div class="container">
        <table class="table">
            <thead>
              <tr>
                <th scope="col">id</th>
                <th scope="col">Nombre</th>
                <th scope="col">Texto</th>
                <th scope="col">Action</th>
              </tr>
            </thead>
            <tbody>
                <% if (arrayMascotas.length > 0) { %>
                    <% arrayMascotas.forEach(mascota => { %>
           
                        <tr>
                            <th scope="row"><%= mascota.id %> </th>
                            <td><%= mascota.nombre %> </td>
                            <td><%= mascota.texto %> </td>
                            <td>@mdo</td>
                          </tr>
                     
                    <% }) %>
           
                   <% } %>
             
            </tbody>
          </table>
      
    </div>


<%- include("template/footer") %>
